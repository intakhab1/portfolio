{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/counter.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/counter",
      "dest": "api/counter.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}